	"sourceMap": "63:2100:0:-;;;173:7;147:33;;297:4;268:33;;429:1;395:35;;63:2100;8:9:-1;5:2;;;30:1;27;20:12;5:2;63:2100:0;;;;;;;"
}//bscoin ICO

//version of compiler

pragma solidity ^0.4.24;

contract bscoin_ico{
    //introducing max number of bscoins available for sale
    uint public max_bscoins = 1000000;
    
    //introducing conversion rate for bs coin against dollar(usd to Bs coin)
    uint public usd_to_bscoins = 1000;
    
    //introducing the total number of coins that have been bought by the investors
    uint public total_bscoins_bought =0;
    
    //mapping from the investor address to it's equity in bs coins and usd
    mapping(address => uint) equity_bscoins;
    
    mapping(address => uint) equity_usd;
    
    //checking if an investor can buy bscoins
    modifier can_buy_bscoins(uint usd_invested){
        require (usd_invested* usd_to_bscoins * total_bscoins_bought <= max_bscoins);
        _;
    }
    
    //getting the equity in bscoins of an investors
    function equity_in_bscoins(address investor)external constant returns (uint){
        return equity_bscoins[investor];   
    }
    
    //getting the equity in USD for investor
    function equity_in_usd(address investor)external constant returns (uint){
        return equity_usd[investor];   
    }
    
    //buying bscoins
    function buy_bscoins(address investor, uint usd_invested) external
    can_buy_bscoins(usd_invested){
        uint bscoins_bought =usd_invested * usd_to_bscoins;
        equity_bscoins[investor]+= bscoins_bought;
        equity_usd[investor] = equity_bscoins[investor] / 1000;
        total_bscoins_bought += bscoins_bought;
    }
    
    //selling bscoins
    function sell_bscoins(address investor, uint bscoins_sold) external{
        equity_bscoins[investor]-= bscoins_sold;
        equity_usd[investor] = equity_bscoins[investor] / 1000;
        total_bscoins_bought -= bscoins_sold;
    }
    
    //selling bscoins
    function transfer_bscoins(address investor,address receiver, uint bscoins_sold) external{
        equity_bscoins[investor]-= bscoins_sold;
        equity_bscoins[receiver]+= bscoins_sold;
        equity_usd[investor] = equity_bscoins[investor] / 1000;
        equity_usd[receiver] = equity_bscoins[receiver] / 1000;
        
    }
    
}
